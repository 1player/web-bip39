{
  "name": "web-bip39",
  "version": "0.0.1",
  "description": "BIP39: Mnemonic code for generating deterministic keys",
  "main": "index.js",
  "module": "dist/web-bip39.esm.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/web-bip39.global.js",
  "sideEffects": false,
  "files": [
    "index.js",
    "dist",
    "wordlists"
  ],
  "scripts": {
    "dev": "vite --config configs/vite.playground.config.ts",
    "build": "npm run build:lib && npm run build:wordlists",
    "build:lib": "rollup -c configs/rollup.lib.config.js",
    "build:wordlists": "node scripts/build-wordlists.js",
    "build:playground": "vite build --config configs/vite.playground.config.ts",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit.js"
  },
  "keywords": [
    "bitcoin",
    "bip39",
    "mnemonic"
  ],
  "author": "Jiulong Hu",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hujiulong/web-bip39.git"
  },
  "bugs": {
    "url": "https://github.com/hujiulong/web-bip39/issues"
  },
  "homepage": "https://github.com/hujiulong/web-bip39#readme",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "antd": "^4.21.4",
    "chalk": "^4.1.2",
    "conventional-changelog-cli": "^2.2.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9",
    "yorkie": "^2.0.0"
  }
}
